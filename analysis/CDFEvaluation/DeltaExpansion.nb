        SuperscriptBox["t", 
         RowBox[{"m", "+", "1"}]], ")"}]}], ",", 
      RowBox[{"{", 
       RowBox[{"t", ",", 
        RowBox[{"{", 
         RowBox[{"tmin", ",", "tmax"}], "}"}], ",", "p", ",", "q"}], "}"}], 
      ",", 
      RowBox[{"MaxIterations", "\[Rule]", "100"}], ",", 
      RowBox[{"WorkingPrecision", "\[Rule]", "400"}]}], "]"}], "[", 
    RowBox[{"[", "2", "]"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"poly", "[", "t_", "]"}], ":=", 
  RowBox[{"Evaluate", "[", " ", 
