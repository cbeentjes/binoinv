   RowBox[{"1", "/", 
    RowBox[{"80", "^", "2"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"minimax", "=", 
   RowBox[{
    RowBox[{"MiniMaxApproximation", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"F", "[", "t", "]"}], " ", "-", " ", 
